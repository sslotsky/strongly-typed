{
  "name": "strongly-typed",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "deploy": "yarn build && yarn clean-dist && yarn build-dist && yarn dist",
    "clean-dist": "rm -r dist/*",
    "build-dist": "yarn parcel build index.html",
    "dist": "cd dist; cp index.html 200.html; surge --domain lavish-balloon.surge.sh"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^4.0.3",
    "bsb-js": "^1.1.7",
    "parcel-bundler": "^1.9.7"
  },
  "main": "index.js",
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "reason-react": "^0.5.3"
  }
}
